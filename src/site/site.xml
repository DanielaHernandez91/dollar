<project name="dollar">
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.1</version>
    </skin>
    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <topBarIcon>
                <name>Maven Fluido Skin</name>
                <alt>Maven Fluido Skin</alt>
                <src>/images/topbar-logo.png</src>
                <href>/index.html</href>
            </topBarIcon>
        </fluidoSkin>
        <reflowSkin>
            <theme>bootswatch-flatly</theme>
            <highlightJs>true</highlightJs>
            <brand>
                <name>Cazcade: Dollar</name>
                <href>http://github.com/neilellis/dollar</href>
            </brand>
            <slogan></slogan>
            <titleTemplate>%2$s | %1$s</titleTemplate>
            <toc>top</toc>
            <topNav>Download|reports|menu1</topNav>
            <bottomNav>
                <column>Main|Download</column>
                <column>Documentation</column>
                <column>dollar|modules</column>
            </bottomNav>
            <bottomDescription>

            </bottomDescription>
            <pages>
                <index project="project-id">
                    <shortTitle>Welcome</shortTitle>
                    <breadcrumbs>true</breadcrumbs>
                    <toc>false</toc>
                    <sections>
                        <carousel/>
                        <body/>
                        <sidebar/>
                        <thumbs>2</thumbs>
                        <columns>3</columns>
                    </sections>
                </index>
                <developer-info>
                    <toc>sidebar</toc>
                </developer-info>
            </pages>
        </reflowSkin>
    </custom>
    <body>
        <links>
            <item name="Intro" href="README.html"/>
            <!--<item name="Page 2" href="http://google.com/"/>-->
            <!--<item name="Page 3" href="http://google.com/"/>-->
        </links>

        <menu name="dollar">
            <!--<item name="Introduction" href="index.html"/>-->
            <!--<item name="Download" href="download.html"/>-->
            <!--<item name="Release Notes" href="release-notes.html"/>-->
            <!--<item name="General Information" href="about.html"/>-->
            <!--<item name="Road Map" href="roadmap.html"/>-->
        </menu>

        <menu ref="reports"/>
    </body>
</project>