
def testArray [ TIME(), {SLEEP(2 Secs);TIME();}, TIME() ];


var a= serial testArray;


var b= parallel testArray;

@@ a
@@ b


//Test different execution orders
.: b[2] < b[1]
.: a[2] >= a[1]

def sleepTime {@@ "Background Sleeping";SLEEP(4 Secs); @@ "Background Finished Sleeping";TIME()}

//Any future reference to c will block until c has completed evaluation
const c= fork sleepTime

SLEEP (1 Sec)

@@ "Main thread sleeping ..."

SLEEP (2 Secs)

@@ "Main thread finished sleeping ..."

const d= TIME()

@@ c
@@ d

.: c > d


