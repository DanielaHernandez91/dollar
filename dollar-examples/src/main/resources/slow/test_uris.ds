const posts = << https://jsonplaceholder.typicode.com/posts

const firstPost= posts[0]
@@ firstPost

const titles= posts each { $1.title }
.: #(titles) != 0

@@titles


volatile a=0
volatile b=0

@@ "100 Millis ="+100 Millis

every 100 Millis until ($1 == 5) unless ($1 == 3) {
    a= a + 1
    @@ "Hello " + a
}

every 100 Millis until ($1 == 5) {
    b= b + 1;
    @@ "Goodbye " + b;
}
