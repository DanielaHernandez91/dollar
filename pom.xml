                            <artifactId>doxia-module-markdown</artifactId>
                            <version>1.3</version>
                        </dependency>
                        <dependency>
                            <groupId>org.apache.maven.skins</groupId>
                            <artifactId>maven-fluido-skin</artifactId>
                            <version>1.3.1</version>
                        </dependency>
                    </dependencies>
                </plugin>

            </plugins>
        </pluginManagement>
        <plugins>

        <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-shade-plugin</artifactId>
                <version>2.3</version>
                <configuration>
                    <shadedArtifactAttached>true</shadedArtifactAttached>
                    <shadedClassifierName>mod</shadedClassifierName>
                    <!--<minimizeJar>true</minimizeJar>-->
                    <artifactSet>
                        <excludes>
                            <exclude>classworlds:classworlds</exclude>
                            <exclude>junit:junit</exclude>
                            <exclude>*:xml-apis</exclude>
                            <exclude>org.apache.maven:lib:tests</exclude>
                            <exclude>log4j:log4j:jar:</exclude>
                            <exclude>org.slf4j:slf4j-api</exclude>
                            <exclude>org.slf4j:slf4j-log4j12</exclude>
                        </excludes>
                    </artifactSet>
                    <filters>
                        <filter>
                            <artifact>*:*</artifact>
                            <excludes>
                                <exclude>org.slf4j.**</exclude>
                                <exclude>org.apache.log4j.**</exclude>
                                <exclude>META-INF/*.SF</exclude>
                                <exclude>META-INF/*.RSA</exclude>
                                <exclude>META-INF/*.INF</exclude>
                                <exclude>META-INF/license</exclude>
                            </excludes>
                        </filter>
                    </filters>
                    <relocations>
                        <relocation>
                            <pattern>me.neilellis.dollar</pattern>
                            <shadedPattern>me.neilellis.dollar.r${buildNumber}</shadedPattern>
                            <excludes>
                                <exclude>me.neilellis.dollar.api.**</exclude>
                                <exclude>me.neilellis.dollar.script.api.**</exclude>
                                <exclude>me.neilellis.dollar.main.**</exclude>
                                <exclude>me.neilellis.dollar.relproxy.**</exclude>
                            </excludes>
                        </relocation>
                        <relocation>
                            <pattern>org</pattern>
                            <shadedPattern>dollar.r${buildNumber}</shadedPattern>
                            <excludes>
                                <exclude>org.apache.log4j.**</exclude>
                                <exclude>org.junit.**</exclude>
                                <exclude>org.w3c.**</exclude>
                                <exclude>org.xml.**</exclude>
                                <exclude>org.slf4j.**</exclude>
                            </excludes>
                        </relocation>
                        <relocation>
                            <pattern>com</pattern>
                            <shadedPattern>dollar.r${buildNumber}</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>net</pattern>
                            <shadedPattern>dollar.r${buildNumber}</shadedPattern>
                            <excludes>
                                <exclude>net.kencochrane.**</exclude>
                            </excludes>
                        </relocation>
                        <relocation>
                            <pattern>io</pattern>
                            <shadedPattern>dollar.r${buildNumber}</shadedPattern>
                        </relocation>
                    </relocations>
                    <transformers>
                        <transformer
                                implementation="org.apache.maven.plugins.shade.resource.ServicesResourceTransformer"/>
                        <transformer
                                implementation="org.apache.maven.plugins.shade.resource.ApacheLicenseResourceTransformer"/>
                        <transformer
                                implementation="org.apache.maven.plugins.shade.resource.ApacheNoticeResourceTransformer">
                            <addHeader>false</addHeader>
                        </transformer>
                    </transformers>
                </configuration>
                <executions>
                    <execution>
                        <phase>package</phase>
                        <goals>
                            <goal>shade</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
