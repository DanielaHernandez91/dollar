marinaVideos = <+ https://itunes.apple.com/search?term=Marina+And+The+Diamonds&entity=musicVideo
videoUrls= each $marinaVideos.results { $1.trackViewUrl }
>> $videoUrls


a=0;
b=0;


every 100 millis until ($1 == 5) unless ($1 == 3) { a= ($a + 1); >> "Hello " + $1; }

every 100 millis until ($1 == 5) { b= $b + 1; >> "Goodbye " + $1; }


sleep 5

$a <=> 3
$b <=> 4



